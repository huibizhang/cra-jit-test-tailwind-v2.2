{
  "version": "0.0.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autoprefixer": "^10.2.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.2.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "npm-run-all -p react-start tailwind-watch",
    "build": "npm-run-all -s tailwind-build react-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "tailwind-watch": "tailwind -i tailwind.css -o ./src/index.css -w",
    "react-build": "react-scripts build",
    "tailwind-build": "tailwind -i tailwind.css -o ./src/index.css --jit --minify"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
